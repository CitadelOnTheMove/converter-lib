<?xml version="1.0" encoding="UTF-8"?>
<main>

	<supported-dataset-formats>
		<!-- Do not change the order -->
		<format>
			<key>csv</key>
			<localization>dataset-format-csv</localization>
		</format>
		<format>
			<key>excel</key>
			<localization>dataset-format-excel</localization>
		</format>
		<format>
			<key>json</key>
			<localization>dataset-format-json</localization>
		</format>
	</supported-dataset-formats>
	
	<format-shared-options>
		<first-rows>
			<first-row>
				<key>data</key>
				<localization>first-row-data</localization>
			</first-row>
			<first-row>
				<key>label</key>
				<localization>first-row-label</localization>
			</first-row>
		</first-rows>
	</format-shared-options>
	
	<csv-options>	
		<delimiters>
			<delimiter>
				<key>\,</key> <!-- comma must be escaped -->
				<localization>csv-delimiter-comma</localization>
			</delimiter>
			<delimiter>
				<key>;</key>
				<localization>csv-delimiter-semicolon</localization>
			</delimiter>
			<delimiter>
				<key>|</key>
				<localization>csv-delimiter-pipe</localization>
			</delimiter>
		</delimiters>
	
		<quotes>
			<quote>
				<key>"</key>
				<localization>csv-quote-dquote</localization>
			</quote>
		</quotes>
		
		<new-lines>
			<new-line>
				<key>\r\n</key>
				<localization>csv-newline-rn</localization>
			</new-line>
			<new-line>
				<key>\r</key>
				<localization>csv-newline-r</localization>
			</new-line>
			<new-line>
				<key>\n</key>
				<localization>csv-newline-n</localization>
			</new-line>
		</new-lines>
	</csv-options>
	
